IF CONTAINS([UPPERCASE Purpose Name], 'AGRI') 
OR CONTAINS([UPPERCASE Purpose Name], 'AGRO')
OR CONTAINS([UPPERCASE Purpose Name], 'AGRA')
OR CONTAINS([UPPERCASE Purpose Name], 'LIVESTOCK')
OR CONTAINS([UPPERCASE Purpose Name], 'HARVEST')
OR CONTAINS([UPPERCASE Purpose Name], 'FERTILIZER')
THEN 'Agriculture'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'WOMEN')
THEN 'Women'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'FOOD')
OR CONTAINS([UPPERCASE Purpose Name], 'NUTR')
THEN 'Food'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'CULTURE')
OR CONTAINS([UPPERCASE Purpose Name], 'SITE')
THEN 'Culture and History'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'NGO')
THEN 'NGOs'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'ENERGY') 
OR CONTAINS([UPPERCASE Purpose Name], 'ELECTRIC')
OR CONTAINS([UPPERCASE Purpose Name], 'COAL') 
OR CONTAINS([UPPERCASE Purpose Name], 'GAS')
OR CONTAINS([UPPERCASE Purpose Name], 'POWER')
OR CONTAINS([UPPERCASE Purpose Name], 'PETROL')
THEN 'Energy'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'TRANS')
OR CONTAINS([UPPERCASE Purpose Name], 'STORAGE')
THEN 'Transportation and Storage'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'COMMUNIC')
OR CONTAINS([UPPERCASE Purpose Name], 'MEDIA')
THEN 'Communication'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'DEBT')
THEN 'Debt'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'TECH')
OR CONTAINS([UPPERCASE Purpose Name], 'ENGINE')
OR CONTAINS([UPPERCASE Purpose Name], 'RESEARCH')
OR CONTAINS([UPPERCASE Purpose Name], 'SECURITY SYSTEM')
THEN 'Science and Technology'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'CIVIL') 
OR CONTAINS([UPPERCASE Purpose Name], 'DEMOC') 
OR CONTAINS([UPPERCASE Purpose Name], 'LEGAL')
OR CONTAINS([UPPERCASE Purpose Name], 'ANTI-CORRUPT')
OR CONTAINS([UPPERCASE Purpose Name], 'GOV')
OR CONTAINS([UPPERCASE Purpose Name], 'ELECT')
OR CONTAINS([UPPERCASE Purpose Name], 'ADMIN')
OR CONTAINS([UPPERCASE Purpose Name], 'PUBLIC')
OR CONTAINS([UPPERCASE Purpose Name], 'FREE FLOW')
OR CONTAINS([UPPERCASE Purpose Name], 'HUMAN RIGHTS')
OR CONTAINS([UPPERCASE Purpose Name], 'SOCIAL/')
OR CONTAINS([UPPERCASE Purpose Name], 'SOCIAL')
OR CONTAINS([UPPERCASE Purpose Name], 'POLITICAL')
OR CONTAINS([UPPERCASE Purpose Name], 'CAPACITY')
THEN 'Democracy, Government, and Civil Society'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'BANK') 
OR CONTAINS([UPPERCASE Purpose Name], 'FIN')
OR CONTAINS([UPPERCASE Purpose Name], 'MONETARY')
THEN 'Finance and Banking'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'FAMILY PLANNING') 
OR CONTAINS([UPPERCASE Purpose Name], 'POPULATION')
THEN 'Population'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'HEALTH') 
OR CONTAINS([UPPERCASE Purpose Name], 'MEDICAL')
OR CONTAINS([UPPERCASE Purpose Name], 'DISEASE')
OR CONTAINS([UPPERCASE Purpose Name], 'MALARIA')
OR CONTAINS([UPPERCASE Purpose Name], 'TUBERCULOSIS')
OR CONTAINS([UPPERCASE Purpose Name], 'PHARMA')
OR CONTAINS([UPPERCASE Purpose Name], 'AIDS')
THEN 'Health and Medical'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'WATER') 
OR CONTAINS([UPPERCASE Purpose Name], 'SANI')
THEN 'Water and Sanitation'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'WASTE')
THEN 'Waste Management'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'EMERGEN') 
OR CONTAINS([UPPERCASE Purpose Name], 'RELIEF')
OR CONTAINS([UPPERCASE Purpose Name], 'BUDGET')
OR CONTAINS([UPPERCASE Purpose Name], 'AID')
OR CONTAINS([UPPERCASE Purpose Name], 'REFUGEES')
OR CONTAINS([UPPERCASE Purpose Name], 'DISASTER')
OR CONTAINS([UPPERCASE Purpose Name], 'FLOOD')
THEN 'Relief, Aid, and Emergency'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'ENVIRON')
OR CONTAINS([UPPERCASE Purpose Name], 'BIO')
THEN 'Environment'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'FISH') 
OR CONTAINS([UPPERCASE Purpose Name], 'FOREST') 
OR CONTAINS([UPPERCASE Purpose Name], 'MINERAL')
OR CONTAINS([UPPERCASE Purpose Name], 'METALS')
OR CONTAINS([UPPERCASE Purpose Name], 'PRODUCTION SECTORS')
THEN 'Natural Resources'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'BUSINESS')
OR CONTAINS([UPPERCASE Purpose Name], 'ECON')
OR CONTAINS([UPPERCASE Purpose Name], 'INDUST')
OR CONTAINS([UPPERCASE Purpose Name], 'EXPORT')
OR CONTAINS([UPPERCASE Purpose Name], 'IMPORT')
OR CONTAINS([UPPERCASE Purpose Name], 'DEVELOPMENT')
OR CONTAINS([UPPERCASE Purpose Name], 'PRIVAT')
OR CONTAINS([UPPERCASE Purpose Name], 'TRADE')
OR CONTAINS([UPPERCASE Purpose Name], 'HOUSING')
OR CONTAINS([UPPERCASE Purpose Name], 'CHEM')
OR CONTAINS([UPPERCASE Purpose Name], 'PLASTER')
OR CONTAINS([UPPERCASE Purpose Name], 'TEXTILE')
OR CONTAINS([UPPERCASE Purpose Name], 'TOURISM')
THEN 'Economic and Industrial Development'

ELSEIF  CONTAINS([UPPERCASE Purpose Name], 'PEACE')
OR CONTAINS([UPPERCASE Purpose Name], 'SOLDIERS')
OR CONTAINS([UPPERCASE Purpose Name], 'LAND MINE')
OR CONTAINS([UPPERCASE Purpose Name], 'SALW')
THEN 'Peace and Conflict'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'EDU') 
OR CONTAINS([UPPERCASE Purpose Name], 'TEACH')
OR CONTAINS([UPPERCASE Purpose Name], 'SKILLS')
OR CONTAINS([UPPERCASE Purpose Name], 'TRAINING')
OR CONTAINS([UPPERCASE Purpose Name], 'DVPT')
OR CONTAINS([UPPERCASE Purpose Name], 'STUDENT')
THEN 'Education and Training'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'NARCOTICS')
THEN 'Crime Prevention'

ELSEIF CONTAINS([UPPERCASE Purpose Name], 'NOT SPECIFIED')
THEN 'Unspecified'

END